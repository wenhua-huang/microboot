<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.microbootrestfulprovider.mapper.MemberMapper">

    <select id="getMember" resultType="com.example.microbootrestfulapi.vo.Member">
          SELECT mid,name,password,locked FROM member WHERE mid=#{mid} ;
